{
  "name": "react-scrollchor",
  "version": "3.0.0",
  "description": "A React component for scroll to #hash links with smooth animations",
  "files": [
    "lib"
  ],
  "repository": "https://github.com/bySabi/react-scrollchor.git",
  "main": "lib/scrollchor.js",
  "engines": {
    "npm": ">=3.0"
  },
  "scripts": {
    "build-npm": "rimraf lib && mkdirp lib && babel src/ -d lib/",
    "lint": "semistandard --fix --verbose | snazzy",
    "testonly": "echo \"Error: no test specified\" && exit 1",
    "test": "npm run lint && npm run testonly",
    "prepare": "npm run build-npm",
    "clean-install": "rm -rf node_modules && rm -rf package-lock.json && npm cache clean --force && npm install"
  },
  "keywords": [
    "react",
    "component",
    "react-component",
    "scroll",
    "anchor",
    "scrollchor",
    "hash",
    "animate"
  ],
  "peerDependencies": {
    "react": ">=15.0.0",
    "fbjs": "*"
  },
  "devDependencies": {
    "babel-cli": "6.x.x",
    "babel-plugin-transform-class-properties": "6.x.x",
    "babel-plugin-transform-export-extensions": "6.x.x",
    "babel-plugin-transform-object-rest-spread": "6.x.x",
    "babel-preset-es2015": "6.x.x",
    "babel-preset-react": "6.x.x",
    "babel-preset-stage-3": "6.x.x",
    "babel-eslint": "7.x.x",
    "prettier-eslint-cli": "4.x.x",
    "semistandard": "*",
    "snazzy": "7.x.x",
    "fbjs": "*",
    "mkdirp": "*",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "rimraf": "2.x.x"
  },
  "semistandard": {
    "parser": "babel-eslint"
  },
  "author": "bySabi Files",
  "license": "ISC"
}
